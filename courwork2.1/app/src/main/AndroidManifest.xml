<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coursework21">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Coursework21">
        <activity android:name=".ImdbActivity"></activity>
        <activity android:name=".RateActivity" />
        <activity android:name=".SearchContainerActivity" />
        <activity android:name=".EditorActivity" />
        <activity android:name=".EditActivity" />
        <activity android:name=".FavouriteActivity" />
        <activity android:name=".DisplayMovie" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>